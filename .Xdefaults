!
! $HOME/.Xdefaults
!
! This file contains user defaults resources which are applied to
! all X-clients.
!
!!!!!!!!!!
!  motif stuff
!!!!!!!!!!
! set false to move only rectangle when window moves
Mwm.screen0.moveOpaque: true
Mwm.screen0.buttonBindings:	DefaultButtonBindings
Mwm.screen0.keyBindings:	DefaultKeyBindings
Mwm.keyboardFocusPolicy:	pointer
Mwm*focusAutoRaise:      False
Mwm*autoRaiseDelay:             400
Mwm.doubleClickTime:	500

Mwm*background: slategray3
Mwm*foreground: black
Mwm*activeBackground: blue
Mwm*activeForeground: white

! Mwm*iconPlacement: top left tight
Mwm*iconPlacement: top left
Mwm*showFeedback: quit restart
Mwm*iconImage: /usr/include/X11/bitmaps/xlogo32
! Mwm*iconDecoration: activelabel label image
Mwm*iconDecoration: activelabel label
Mwm*useClientIcon: True
Mwm*iconPlacementMargin: 5

Mwm*menu*fontList: -adobe-helvetica-medium-r-*-*-14-*-*-*-*-*-*-*
Mwm*title*fontList: -adobe-helvetica-medium-r-*-*-14-*-*-*-*-*-*-*
Mwm*icon*fontList: -adobe-helvetica-medium-r-*-*-12-*-*-*-*-*-*-*

Mwm*Emacs*iconImage: /usr/include/X11/bitmaps/terminal
Mwm*XClock.clientDecoration: menu border
Mwm*XLoad.clientDecoration: menu border
Mwm*XBiff.clientDecoration: menu border


!!!!!!!!!!
!  xterm
!!!!!!!!!!
XTerm*geometry: 90x40
XTerm*saveLines: 2048
XTerm*scrollBar: on
XTerm*scrollKey: on

XTerm*font: -adobe-courier-medium-r-*-*-12-*-*-*-*-*-*-*
XTerm*font2: -adobe-courier-medium-r-*-*-8-*-*-*-*-*-*-*
XTerm*font3: -adobe-courier-medium-r-*-*-10-*-*-*-*-*-*-*
XTerm*font4: -adobe-courier-medium-r-*-*-14-*-*-*-*-*-*-*
XTerm*font5: -adobe-courier-medium-r-*-*-18-*-*-*-*-*-*-*
XTerm*font6: -adobe-courier-medium-r-*-*-24-*-*-*-*-*-*-*

!! needed for Meta to work right in bash's cmdline editing
XTerm*eightBitInput: false

!  Character re-mapping ?
XTerm*VT100.Translations: #override \n\
  <Key>F4: string("exit\n") \n\
  <Key>F5: string("exit\n")

XTerm*background: wheat1
XTerm*foreground: Black

!!! "White" are unused. This is so if they are used somewhere, I'll notice.
!! recview background (moneyline)
!XTerm*color0: wheat1
!! ls zipped files (suse) and vi notice background (suse)
!XTerm*color1: Black
!! ls executables (suse)
!XTerm*color2: Blue
!XTerm*color3: White
!XTerm*color4: White
!XTerm*color5: White
!! ls symlinks (suse)
!XTerm*color6: grey50
!XTerm*color7: White
!XTerm*color8: White
!! ls zipped files, vi misc (suse), man bold (suse)
!XTerm*color9: Black
!! ls executables
!XTerm*color10: Blue
!! recview status line (moneyline)
!XTerm*color11: Red
!! ls directories
!XTerm*color12: ForestGreen
!! ls images (suse)
!XTerm*color13: Black
!! ls symlinks
!XTerm*color14: grey50
!! vi notice foreground (suse), recview image (moneyline)
!XTerm*color15: Red

!!!!!!!!!!
!  rxvt (for cygwin)
!!!!!!!!!!
Rxvt*geometry: 90x32
Rxvt*font: Courier New-15
Rxvt*background: lightblue1
! VIM-like colors
Rxvt*color0:    #000000
!Rxvt*color1:    #FFFFFF
Rxvt*color1:    #000000
Rxvt*color2:    #00A800
Rxvt*color3:    #FFFF00
Rxvt*color4:    #0000A8
Rxvt*color5:    #A800A8
Rxvt*color6:    #00A8A8
Rxvt*color7:    #D8D8D8
Rxvt*color8:    #000000
!Rxvt*color9:    #FFFFFF
Rxvt*color9:    #000000
Rxvt*color10:   #00A800
Rxvt*color11:   #FFFF00
Rxvt*color12:   #0000A8
Rxvt*color13:   #A800A8
Rxvt*color14:   #00A8A8
Rxvt*color15:   #D8D8D8

!!!!!!!!!!
!  other misc apps
!!!!!!!!!!
*Background: wheat1
*Foreground: Black

Emacs.font: -adobe-courier-medium-r-*-*-12-*-*-*-*-*-*-*
Emacs.geometry: 85x45+0+0

XClock.geometry: 100x95+0-0
XLoad.geometry: 100x95
XBiff.geometry: 60x60+0-40

XBiff.mailbox.emptyPixmap: noletters
XBiff.mailbox.fullPixmap: letters
XBiff.mailbox.flip: false
XBiff*Motif.SelectColor: yellow

Mxdb*background: slategray3
Mxdb*fontList: -adobe-courier-medium-r-*-*-10-*-*-*-*-*-*-*=normal, -adobe-courier-bold-r-*-*-10-*-*-*-*-*-hp-*=bold, -adobe-courier-medium-o-*-*-10-*-*-*-*-*-hp-*=italic

