!
! $HOME/.Xdefaults
!
! This file contains user defaults resources which are applied to
! all X-clients.
!
!!!!!!!!!!
!  motif stuff
!!!!!!!!!!
! set false to move only rectangle when window moves
Mwm.screen0.moveOpaque: true
Mwm.screen0.buttonBindings:	DefaultButtonBindings
Mwm.screen0.keyBindings:	DefaultKeyBindings
Mwm.keyboardFocusPolicy:	pointer
Mwm*focusAutoRaise:      False
Mwm*autoRaiseDelay:             400
Mwm.doubleClickTime:	500

Mwm*background: slategray3
Mwm*foreground: black
Mwm*activeBackground: blue
Mwm*activeForeground: white

! Mwm*iconPlacement: top left tight
Mwm*iconPlacement: top left
Mwm*showFeedback: quit restart
Mwm*iconImage: /usr/include/X11/bitmaps/xlogo32
! Mwm*iconDecoration: activelabel label image
Mwm*iconDecoration: activelabel label
Mwm*useClientIcon: True
Mwm*iconPlacementMargin: 5

Mwm*menu*fontList: -adobe-helvetica-medium-r-*-*-14-*-*-*-*-*-hp-*
Mwm*title*fontList: -adobe-helvetica-medium-r-*-*-14-*-*-*-*-*-hp-*
Mwm*icon*fontList: -adobe-helvetica-medium-r-*-*-12-*-*-*-*-*-hp-*

Mwm*Emacs*iconImage: /usr/include/X11/bitmaps/terminal
Mwm*XClock.clientDecoration: menu border
Mwm*XLoad.clientDecoration: menu border
Mwm*XBiff.clientDecoration: menu border


!!!!!!!!!!
!  xterm
!!!!!!!!!!
XTerm*geometry: 100x55
XTerm*saveLines: 2048
XTerm*scrollBar: on
XTerm*scrollKey: on

XTerm*font: -adobe-courier-medium-r-*-*-12-*-*-*-*-*-hp-*
XTerm*font2: -adobe-courier-medium-r-*-*-8-*-*-*-*-*-hp-*
XTerm*font3: -adobe-courier-medium-r-*-*-10-*-*-*-*-*-hp-*
XTerm*font4: -adobe-courier-medium-r-*-*-14-*-*-*-*-*-hp-*
XTerm*font5: -adobe-courier-medium-r-*-*-18-*-*-*-*-*-hp-*
XTerm*font6: -adobe-courier-medium-r-*-*-24-*-*-*-*-*-hp-*

!! needed for Meta to work right in bash's cmdline editing
XTerm*eightBitInput: false

!  Character re-mapping ?
XTerm*VT100.Translations: #override \n\
  <Key>F4: string("exit\n") \n\
  <Key>F5: string("exit\n")

XTerm*background: wheat1
XTerm*foreground: Black

Rxvt*color10: Cyan4

!!!!!!!!!!
!  other misc apps
!!!!!!!!!!
*Background: wheat1
*Foreground: Black

Emacs.font: -adobe-courier-medium-r-*-*-12-*-*-*-*-*-hp-*
Emacs.geometry: 85x60+0+0

XClock.geometry: 100x95+0-0
XLoad.geometry: 100x95
XBiff.geometry: 60x60+0-40

XBiff.mailbox.emptyPixmap: noletters
XBiff.mailbox.fullPixmap: letters
XBiff.mailbox.flip: false
XBiff*Motif.SelectColor: yellow

Mxdb*background: slategray3
Mxdb*fontList: -adobe-courier-medium-r-*-*-10-*-*-*-*-*-hp-*=normal, -adobe-courier-bold-r-*-*-10-*-*-*-*-*-hp-*=bold, -adobe-courier-medium-o-*-*-10-*-*-*-*-*-hp-*=italic

!  <Key>Up: string("") \n\
!  <Key>Down: string("") \n\
!  <Key>Left: string("") \n\
!  <Key>Right: string("") \n\
!  <Key>Tab: string("")

