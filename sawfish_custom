;; sawfish user customization -- do not edit by hand!
;; sawfish version 1.3, written Thu May  6 14:48:03 2004

(custom-set-typed-variable (quote default-frame-style) (quote simple) (quote frame-style))
(custom-set-typed-variable (quote simple:active-color) (quote "#dddd44434443") (quote color))
(custom-set-typed-variable (quote simple:normal-color) (quote "#77777777cccc") (quote color))
(custom-set-typed-variable (quote cycle-all-viewports) (quote t) (quote boolean))
(custom-set-typed-variable (quote cycle-all-workspaces) (quote t) (quote boolean))
(custom-set-typed-variable (quote focus-windows-when-mapped) (quote ()) (quote boolean))
(custom-set-typed-variable (quote focus-mode) (quote enter-exit) (quote symbol))
