[Desktop Entry]
Encoding=UTF-8
Type=Application
Name=Folding@home Client
Exec=FAHClient --send-command unpause
Categories=Education;Science;Biology;;
Comment=Folding@home Client Control
Icon=/usr/share/pixmaps/FAHClient.png
Actions=stop;finish

[Desktop Action stop]
Name=Stop Folding@home Client (Pause)
Exec=FAHClient --send-command pause

[Desktop Action finish]
Name=Finish Folding@home Client Processing
Exec=FAHClient --send-command finish
