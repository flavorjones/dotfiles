#! /bin/bash

cd $HOME

rm -f .bashrc
ln -s ${HOME}/dotfiles/.bashrc .

rm -f bin/clup
ln -s ${HOME}/dotfiles/clup bin

rm -f .emacs
ln -s ${HOME}/dotfiles/.emacs .

rm -f .exrc
ln -s ${HOME}/dotfiles/.exrc .

rm -f .mwmrc
ln -s ${HOME}/dotfiles/.mwmrc .

rm -f .twmrc
ln -s ${HOME}/dotfiles/.twmrc .

rm -f .Xdefaults
ln -s ${HOME}/dotfiles/.Xdefaults .

rm -f .xmodmap4win
ln -s ${HOME}/dotfiles/.xmodmap4win .

rm -f .inputrc
ln -s ${HOME}/dotfiles/.inputrc .

rm -f .sawfishrc
ln -s ${HOME}/dotfiles/.sawfishrc
